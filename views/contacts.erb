<article>There are <span><%= @contacts.length %></span> contacts.</article>
<% @contacts.each do |contact| -%>
  <ul>
    <li>
      <h3><%= contact.full_name %> (<%= contact.email %>)</h3>
      <p>
        <a href=<%= "" %> >Edit</a> |
        <form method="post" action=<%= "contacts/#{contact.id}" %> >
          <input type='hidden' name='_method' value='delete'>
          <button type="submit" name="button">Delete top contact</button>
        </form>
      </p>
      <p><a href=<%= "contacts/#{contact.id}" %> >Contact Info</a></p>
    </li>
  </ul>
<% end %>
